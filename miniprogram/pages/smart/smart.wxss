.page{
    padding:10px;
}
.info{
    border-radius: 10px;
    margin-bottom:10px;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    height: 120px;
    position: relative;
    overflow: hidden;
}
.imgbox{
    width: 40px;
    height: 40px;
    border-radius: 50%;
    overflow: hidden;
    border: 2px #fff solid;
}
.img{
    width: 100%;
    height: 100%;
}
.name{
    color: #fff;
    font-size: 12px;
    text-indent: 5px;
}
.welcome{
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    font-size: 20px;
    font-weight: 500;
    color: #fff;
    letter-spacing: 5px;
}
.contentbox{
    padding: 10px 0;
    background: black;
    border-radius: 8px;
    display: flex;
    justify-content: space-around;
    flex-wrap: wrap;
    margin-top: 10px;
}
.last{
    color: #fff;
}
.item{
    width:28%;
    padding: 8px;
    font-size: 12px;
    background: blueviolet;
    margin:10px;
    color: #fff;
    border-radius: 10px;
    border:2px #FFF solid;
    text-align: center;
    letter-spacing: 1px;
}
.seconditem{
    background: orange;
}
.item:active{
    background: #fff;
    border: 1px blueviolet solid;
    color: blueviolet;
}
.seconditem:active{
    background: #fff;
    border: 1px orange solid;
    color: orange;
}